type Publisher {
    publisherId: Int!
    name: String!
    street: String!
    city: String!
    state: String!
    postalCode: String!
    phone: String!
    email: String!
    books: [Book]
}
type Book {
    bookId: Int!
    isbn: String!
    title: String!
    publishDate: String!
    authorId: Int!
    publisherId: Int!
    price: String!
    author : Author
    publisher: Publisher
}
type Author {
     authorId: Int!
     firstName: String!
     lastName: String!
     street: String!
     city: String!
     state: String!
     postalCode: String!
     phone: String!
     email: String!
    books: [Book]
}
type Query {
    findPublisherById(id:Int!): Publisher
    findAuthorById(id:Int!): Author
    findBookById(id:Int!): Book

}
